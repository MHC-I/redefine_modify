<div class="tag-container shadow-none hover:shadow-none sm:shadow-redefine sm:hover:shadow-redefine-hover">
	<div class="tag-name">
		<i class="fa-solid fa-tag"></i>&nbsp;<%= page.tag %>
	</div>
	<div class="tag-post-list">
		<%- partial('_widgets/archive-list', {posts_new: page.posts}) %>
